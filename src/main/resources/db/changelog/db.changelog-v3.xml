<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet id="V3.0.0" author="dxl">
        <comment>用户扫码记录</comment>
        <createTable tableName="sys_user" remarks="管理员表">
            <column name="su_id" type="bigint" autoIncrement="true" remarks="主键">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="su_login_name" type="varchar(128)" remarks="用户名"/>
            <column name="su_password" type="varchar(128)" remarks="密码"/>
            <column name="su_create_time" type="bigint(20)" remarks="创建时间"/>
            <column name="su_update_time" type="bigint(20)" remarks="修改时间"/>
        </createTable>
    </changeSet>
</databaseChangeLog>
