<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet id="V8.0.0" author="dxl">
        <comment>创建浏览记录表</comment>
        <createTable tableName="prad_view_record" remarks="浏览记录表">
            <column name="vr_id" type="bigint" autoIncrement="true" remarks="主键">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="vr_user_id" type="bigint" remarks="用户主键"/>
            <column name="vr_news_id" type="bigint" remarks="课程主键"/>
            <column name="vr_time" type="bigint" remarks="浏览时间"/>
        </createTable>
    </changeSet>
</databaseChangeLog>

