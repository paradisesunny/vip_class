<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet id="V9.0.0" author="dxl">
        <comment>创建访问记录日志表</comment>
        <createTable tableName="prad_visit_log" remarks="访问记录日志">
            <column name="vl_id" type="bigint" autoIncrement="true" remarks="主键">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="vl_user_id" type="bigint" remarks="用户主键"/>
            <column name="vl_start_time" type="bigint" remarks="访问页面开始时间"/>
            <column name="vl_end_time" type="bigint" remarks="访问页面结束时间"/>
            <column name="vl_duration" type="bigint" remarks="访问页面时长(秒)"/>
            <column name="vl_visit_module" type="varchar(64)" remarks="观看模块"/>
            <column name="vl_news_module" type="varchar(64)" remarks="资讯所属模块"/>
            <column name="vl_news_id" type="bigint" remarks="资讯主键"/>
            <column name="vl_include_video" type="int" remarks="是否包含视频(0：不包含，1：包含)"/>
            <column name="vl_ip" type="varchar(128)" remarks="ip地址"/>
            <column name="vl_like" type="int" remarks="是否点赞(0：未点赞；1: 点赞；2：取消点赞)"/>
            <column name="vl_like_time" type="bigint" remarks="点赞时间/取消点赞时间"/>
            <column name="vl_favorite" type="int" remarks="是否收藏(0：未收藏；1: 收藏；2：取消收藏)"/>
            <column name="vl_favorite_time" type="bigint" remarks="收藏时间/取消收藏时间"/>
            <column name="vl_vr_id" type="bigint" remarks="视频观看记录主键"/>
        </createTable>
    </changeSet>
</databaseChangeLog>

